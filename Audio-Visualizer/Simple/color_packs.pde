
public class ColorPack {
  color bg;
  color waveform;
  color stroke;
  color alt;
  color levels;
  color timeLine;
  color timeFill;
  color low;
  color mid;
  color high;

  public ColorPack( color nbg, color nwaveform, color nstroke, color nalt, 
    color nlevels, color ntimeLine, color ntimeFill, 
    color nlow, color nmid, color nhigh) {
      bg = nbg;
      waveform = nwaveform;
      stroke = nstroke;
      alt = nalt;
      levels = nlevels;
      timeLine = ntimeLine;
      timeFill = ntimeFill;
      low = nlow;
      mid = nmid;
      high = nhigh;
  }
}
